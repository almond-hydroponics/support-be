{
  "name": "support-be",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run tsc",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx ",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn prettier",
    "prettier": "prettier 'src/**/*.ts'",
    "prettier:fix": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "deploy": "exit 0",
    "prepare": "husky install",
    "test:unit": "cross-env NODE_ENV=test jest --runInBand --coverage"
  },
  "author": "Mfuon",
  "license": "MIT",
  "dependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@hapi/joi": "^17.1.1",
    "@sideway/address": "^4.1.2",
    "@sideway/formula": "^3.0.0",
    "@sideway/pinpoint": "^2.0.0",
    "@tsed/ajv": "^6.62.2",
    "@tsed/common": "^6.90.0",
    "@tsed/core": "^6.62.2",
    "@tsed/di": "^6.62.2",
    "@tsed/exceptions": "^6.62.4",
    "@tsed/json-mapper": "^6.62.2",
    "@tsed/mongoose": "^6.62.2",
    "@tsed/passport": "^6.62.2",
    "@tsed/platform-express": "^6.62.2",
    "@tsed/schema": "^6.62.2",
    "@tsed/socketio": "^6.62.2",
    "@tsed/swagger": "^6.62.3",
    "@tsed/typeorm": "^6.62.2",
    "@types/swagger-schema-official": "^2.0.22",
    "ajv": "^8.6.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "express": "^4.17.1",
    "express-serve-static-core": "^0.1.1",
    "jest-when": "^3.4.0",
    "method-override": "^3.0.0",
    "mongodb": "^4.1.0",
    "mongoose": "^5.13.7",
    "nodemon": "^2.0.12",
    "nvm": "^0.0.4",
    "passport": "^0.4.1",
    "socket.io": "^4.1.3",
    "ts-jest": "^27.0.5",
    "typeorm": "^0.2.36",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "3.5.0",
    "@tsed/cli-plugin-jest": "3.5.0",
    "@tsed/cli-plugin-passport": "3.5.0",
    "@tsed/cli-plugin-typeorm": "3.5.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-unused-imports": "latest",
    "husky": "^7.0.0",
    "jest": "^27.2.0",
    "jest-config": "^27.1.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "supertest": "^6.1.6"
  },
  "tsed": {
    "packageManager": "yarn",
    "convention": "default"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run prettier:fix && yarn run lint:fix",
      "pre-push": ""
    }
  }
}
